{
  "name":"change a shortcut",
  "manifest_version":2,
  "version":"0.1",
  "description":"try the command api",

  "browser_action":{
    "default_title": "changeshortcut"
  },

  "background":{
    "scripts":["background.js"],
    "persistent":false
  },

  "commands":{
    "toggle-feature-foo": {
        "suggested_key": {
          "default": "Ctrl+Shift+U"
        },
        "description": "jump to expage"
      }
   },

   "permissions":[
     "tabs"
   ]
}
