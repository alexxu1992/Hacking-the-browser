{
  "manifest_version":2,
  "name":"my extensions",
  "version":"1.0",

  "browser_action":{
    "default_title":"Hello ,world"
  },

  "content_scripts":[
    {
      "matches": ["<all_urls>"],
      "css": ["mystyles.css"],
      "js":["content.js"]
    }
  ],

  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],

  "web_accessible_resources": [
    "cat.jpg"
  ],

  "background":{
    "scripts":["background.js"]
  }
}
